{
  "name": "doki-master-theme",
  "packageManager": "yarn@4.12.0",
  "private": true,
  "workspaces": [
    "doki-build-source/",
    "buildSrc/",
    "../doki-build-source"
  ],
  "scripts": {
    "getDokiBuildSrc": "git clone https://github.com/ZimCodes/doki-build-source.git",
    "buildDokiSrc": "yarn workspace doki-build-source build",
    "generateTemplates": "yarn workspace master-theme-build generateTemplates",
    "generateAssets": "yarn workspace master-theme-build generateAssets",
    "generateColors": "yarn workspace master-theme-build generateColors",
    "generateSmolAssets": "yarn workspace master-theme-build generateSmolAssets",
    "modifyThemes": "yarn workspace master-theme-build modifyThemes",
    "cleanAssets": "yarn workspace master-theme-build cleanAssets",
    "cleanTemplates": "yarn workspace master-theme-build cleanTemplates",
    "generateAll": "yarn generateTemplates && yarn generateSmolAssets && yarn generateColors"
  }
}
